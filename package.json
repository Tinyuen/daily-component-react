  "description": "daily react components",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "cross-env NODE_ENV=development node_modules/.bin/webpack-dev-server --config scripts/webpack/webpack.dev.config.js",
    "clean": "rimraf es lib dist",
    "build:css": "",
    "build:es": "babel src -d es --extensions \".ts,.tsx\" --copy-files --no-babelrc --config-file scripts/babel/config.esm.js",
    "build:commonjs": "babel src -d lib --extensions \".ts,.tsx\" --copy-files --no-babelrc --config-file scripts/babel/config.commonjs.js",
    "build:types": "",
    "build:umd": "node_modules/.bin/webpack --config scripts/webpack/webpack.config.umd.js && cross-env NODE_ENV=production node_modules/.bin/webpack --config scripts/webpack/webpack.config.umd.js",
    "build:all": "npm run clean && npm run build:umd && npm run build:commonjs && npm run build:es"
  },
