{
  "name": "@tinyuen/ui",
  "author": "tinyuen",
  "version": "0.0.3",
  "description": "daily react components",
  "keywords": [
    "react",
    "component",
    "react-component",
    "frontend",
    "components",
    "ui"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "cross-env NODE_ENV=development node_modules/.bin/webpack-dev-server --config scripts/webpack/webpack.dev.config.js",
    "clean": "rimraf es lib dist types",
    "build:css": "gulp compile -f ./scripts/gulp/gulpfile.js",
    "build:es": "babel components -d es --extensions \".ts,.tsx\" --copy-files --no-babelrc --config-file ./scripts/babel/config.esm.js",
    "build:commonjs": "babel components -d lib --extensions \".ts,.tsx\" --copy-files --no-babelrc --config-file ./scripts/babel/config.commonjs.js",
    "build:types": "tsc --outDir types -d --emitDeclarationOnly",
    "build:umd": "node_modules/.bin/webpack --config scripts/webpack/webpack.config.umd.js && cross-env NODE_ENV=production node_modules/.bin/webpack --config ./scripts/webpack/webpack.config.umd.js",
    "build:all": "npm run clean && npm run build:umd && npm run build:commonjs && npm run build:es && npm run build:css",
    "pub": "npm publish --access public"
  },
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "dist",
    "es",
    "lib",
    "types"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tinyuen/daily-component-react.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Tinyuen/daily-component-react/issues"
  },
  "homepage": "https://github.com/Tinyuen/daily-component-react#readme",
  "devDependencies": {
